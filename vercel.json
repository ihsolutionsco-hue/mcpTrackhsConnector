{
  "version": 2,
  "builds": [
    {
      "src": "src/trackhs_mcp/server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/trackhs_mcp/server.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12"
  },
  "functions": {
    "src/trackhs_mcp/server.py": {
      "runtime": "python3.12"
    }
  }
}
