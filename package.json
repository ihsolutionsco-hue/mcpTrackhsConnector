{
  "name": "trackhs-mcp-connector",
  "version": "1.0.0",
  "type": "module",
  "description": "MCP Remote Connector para Track HS API - Desplegado en Vercel",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:vercel": "vercel dev",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "setup:vercel": "tsx scripts/vercel-setup.ts",
    "clean": "rm -rf dist",
    "setup": "node scripts/setup.js",
    "test": "jest",
    "test:unit": "jest tests/unit/",
    "test:integration": "jest tests/integration/",
    "test:e2e": "jest tests/e2e/",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=2",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:performance": "jest tests/e2e/performance.test.ts --testTimeout=60000",
    "test:security": "jest tests/e2e/security.test.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "keywords": [
    "mcp",
    "trackhs",
    "api",
    "reviews",
    "reservations",
    "contacts",
    "ledger-accounts",
    "accounting",
    "finance",
    "maintenance",
    "work-orders",
    "vercel",
    "remote-connector",
    "serverless"
  ],
  "author": "Track HS Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^17.2.3",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.0",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vercel": "^48.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
