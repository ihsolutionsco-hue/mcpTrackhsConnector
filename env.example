# Configuración de TrackHS MCP Connector
# Copia este archivo como .env y configura tus credenciales

# Credenciales de TrackHS (REQUERIDAS)
TRACKHS_USERNAME=tu_usuario_aqui
TRACKHS_PASSWORD=tu_contrasena_aqui

# URL de la API de TrackHS (opcional, usa el valor por defecto si no se especifica)
TRACKHS_API_URL=https://ihmvacations.trackhs.com

# Configuración del servidor (opcional)
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# Nivel de logging (opcional)
LOG_LEVEL=INFO

# Timeout de API en segundos (opcional)
API_TIMEOUT=30
