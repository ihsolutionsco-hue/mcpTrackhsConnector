{
  "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
  "source": {
    "path": "src/__main__.py"
  },
  "environment": {
    "type": "uv",
    "python": ">=3.11",
    "dependencies": [
      "fastmcp>=2.13.0",
      "httpx>=0.27.0",
      "pydantic>=2.12.3",
      "python-dotenv>=1.0.1"
    ]
  },
  "deployment": {
    "transport": "http",
    "port": 8080
  },
  "secrets": {
    "required": [
      "TRACKHS_API_URL",
      "TRACKHS_USERNAME",
      "TRACKHS_PASSWORD"
    ]
  }
}
