{
  "name": "TrackHS API",
  "version": "2.0.0",
  "description": "TrackHS MCP Server - Servidor MCP para TrackHS API",
  "transport": "http",
  "host": "0.0.0.0",
  "port": 8000,
  "entrypoint": "src/trackhs_mcp/__main__.py",
  "python_version": "3.12",
  "dependencies": [
    "fastmcp>=2.13.0",
    "httpx>=0.27.0",
    "python-dotenv>=1.0.1",
    "pydantic>=2.12.3"
  ],
  "environment_variables": {
    "TRACKHS_USERNAME": {
      "required": true,
      "description": "Usuario de TrackHS API"
    },
    "TRACKHS_PASSWORD": {
      "required": true,
      "description": "Contrase√±a de TrackHS API"
    },
    "TRACKHS_API_URL": {
      "required": false,
      "default": "https://ihmvacations.trackhs.com",
      "description": "URL base de la API TrackHS"
    },
    "LOG_LEVEL": {
      "required": false,
      "default": "INFO",
      "description": "Nivel de logging"
    }
  },
  "health_check": {
    "enabled": true,
    "endpoint": "/health",
    "timeout": 30
  },
  "cors": {
    "enabled": true,
    "origins": [
      "https://elevenlabs.io",
      "https://api.elevenlabs.io",
      "https://app.elevenlabs.io",
      "https://claude.ai",
      "https://app.claude.ai"
    ]
  }
}
